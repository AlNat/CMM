int main ( void ) {
	
	/* Комментарий */

	/* Пробелы обязательны везде! После отрывающиейся скобки и перд закрывающейся, между арифметическими выраениями и тд */
	/* Точка с запятой стоит без пробела за последним символом выражения */

	/* Инициализация переменных: */
	/* Все пеерменные долны быть инициализированы при создании */

	/* */

	out ( var ); /* Пробнлы обязательны! */



	int a = 3;  	/* Целочисленные переменные */
	double d = 2.2; 	/* Дробные переменные. Обязтальна полная инициализация */

	/* Арифметика */

    out ( a );

	a = 12;
	
	a = a + 4;
	a = a / 8;
	a = a * 3;
	a = a - 2;
	

	/* Условные выражения */
	if ( a == 12 ) { 	/* В if else всегда должно быть полное описание */
		out ( a ); 	
	} 

	while ( d < 4 ) {
		d = d - 0.5;
		a = a - 1; /* ++ и -- не поддериваються */
	}

	return 0;
}